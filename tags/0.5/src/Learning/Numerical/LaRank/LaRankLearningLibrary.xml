<?xml version="1.0" encoding="UTF-8"?>

<library name="LaRankLearning" directory="Learning/Numerical/LaRank">

  <enumeration name="LaRankKernelType">
    <value name="linear"/>
    <value name="polynomial"/>
    <value name="rbf"/>
  </enumeration>

  <class name="LaRankLearningMachine" base="Function" abstract="yes">
    <variable type="Double" name="C"/>
    <variable type="LaRankKernelType" name="kernelType"/>
    <variable type="PositiveInteger" name="kernelDegree"/>
    <variable type="Double" name="kernelGamma"/>
    <variable type="Double" name="kernelCoef0"/>
  </class>

  <class name="LaRankBatchLearner" base="BatchLearner">
    <constructor/>
  </class>

  <class name="LaRankClassifier" base="LaRankLearningMachine">
    <constructor arguments="double C, LaRankKernelType kernelType, size_t kernelDegree, double kernelGamma, double kernelCoef0" returnType="Function"/>
  </class>

  <class name="LaRankBinaryClassifier" base="LaRankClassifier">
    <constructor arguments="double C, LaRankKernelType kernelType, size_t kernelDegree, double kernelGamma, double kernelCoef0" returnType="Function"/>
  </class>

</library>
