<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinLuape" directory="Luape">

  <!-- Protein Perception -->
  <class name="ProteinResiduePerception" base="Object">
    <variable type="ProteinPerception" name="protein"/>
    <variable type="PositiveInteger" name="position"/>
<!--    
    <variable type="ProteinResiduePerception" name="previous" shortName="prev"/>
    <variable type="ProteinResiduePerception" name="next"/> -->
    
    <variable type="ProteinResiduePerception" name="previousOfType" shortName="prevT"/>
    <variable type="ProteinResiduePerception" name="nextOfType" shortName="nextT"/>
    <variable type="PositiveInteger" name="positionOfType"/>

    <variable type="AminoAcidType" name="aminoAcidType" enumeration="yes"/>
    <variable type="DenseDoubleVector[PositionSpecificScoringMatrix, Probability]" name="pssmRow"/>
    <variable type="DenseDoubleVector[SecondaryStructureElement, Probability]" name="secondaryStructure"/>
    <variable type="DenseDoubleVector[DSSPSecondaryStructureElement, Probability]" name="dsspSecondaryStructure"/> 
    <variable type="DenseDoubleVector[StructuralAlphabetElement, Probability]" name="structuralAlphabet"/>
    <variable type="Probability" name="solventAccessibilityAt20p"/>
    <variable type="Probability" name="disordered"/>
  </class>
  
  <class name="ProteinResiduePairPerception" base="Object">
    <variable type="ProteinPerception" name="protein"/>
    <variable type="ProteinResiduePerception" name="firstResidue" shortName="r1"/>
    <variable type="ProteinResiduePerception" name="secondResidue" shortName="r2"/>
  </class>

  <class name="ProteinPerception" base="Object">
    <variable type="ObjectVector[ProteinResiduePerception]" name="residues"/>
  </class>

  <class name="ProteinGetRelativeResidueLuapeFunction" base="LuapeFunction">
    <variable type="Integer" name="delta"/>
  </class>

  <!-- Protein Predictor Parameters -->
  <class name="DisulfideBondClassifier" base="LuapeBinaryClassifier"/>
  
  <class name="LuapeProteinPredictorParameters" base="ProteinPredictorParameters">
    <variable type="PositiveInteger" name="treeDepth"/>
    <variable type="PositiveInteger" name="complexity"/>
    <variable type="Double" name="relativeBudget"/>
    <variable type="Double" name="miniBatchRelativeSize"/>
    <variable type="PositiveInteger" name="numIterations"/>
    <variable type="Boolean" name="verbose"/>
  </class>

  <!-- Sand Box -->
  <class name="ProteinLuapeSandBox" base="WorkUnit">
    <variable type="File" name="trainingInputDirectory"/>
    <variable type="File" name="trainingSupervisionDirectory"/>
    <variable type="File" name="testingInputDirectory"/>
    <variable type="File" name="testingSupervisionDirectory"/>
    <variable type="PositiveInteger" name="maxProteinCount"/>

    <variable type="PositiveInteger" name="treeDepth"/>
    <variable type="PositiveInteger" name="complexity"/>
    <variable type="Double" name="relativeBudget"/>
    <variable type="Double" name="miniBatchRelativeSize"/>
    <variable type="PositiveInteger" name="numIterations"/>
  </class>
  
  <!-- DSB Formula Discovery
  <class name="DSBFormulaSearchProblem" base="LuapeNodeSearchProblem">
    <variable type="File" name="trainingInputDirectory"/>
    <variable type="File" name="trainingSupervisionDirectory"/>
    <variable type="File" name="testingInputDirectory"/>
    <variable type="File" name="testingSupervisionDirectory"/>
    <variable type="PositiveInteger" name="maxProteinCount"/>    
  </class>
   -->
</library>
