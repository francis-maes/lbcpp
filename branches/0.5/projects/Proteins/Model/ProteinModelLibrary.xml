<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinModel" directory="Model">

  <!-- Protein Model -->
  <class name="Model" base="CompositeFunction" abstract="yes"/>

  <class name="ProteinModel" base="Model" abstract="yes">
    <variable type="VariablesEnumeration[Protein]" name="target"/>
  </class>

  <class name="ProteinParallelModel" base="CompositeFunction">
    <variable type="ObjectVector[ProteinModel]" name="models"/>
  </class>

  <class name="ProteinSequentialModel" base="CompositeFunction">
    <variable type="ObjectVector[CompositeFunction]" name="models"/>
  </class>

  <!-- Protein Map -->
  <class name="Map" base="Object"/>

  <class name="ProteinMap" base="Map">
    <variable type="Protein" name="protein"/>
    <variable type="PositiveInteger" name="length"/>
  </class>

  <class name="CreateProteinMap" base="SimpleUnaryFunction"/>
  <class name="GetProteinMapElement" base="Function"/>
  <class name="GetSimpleProteinMapElement" base="GetProteinMapElement"/>




  <!-- Simple Model -->
  <class name="SimpleProteinModel" base="ProteinModel">
    <!-- Machine Learning -->
    <variable type="PositiveInteger" name="x3Trees"/>
    <variable type="PositiveInteger" name="x3Attributes"/>
    <variable type="PositiveInteger" name="x3Splits"/>
    <variable type="Boolean" name="x3LowMemory"/>

    <!-- Global Parameters -->
    <variable type="Boolean" name="useProteinLength"/>
    <variable type="Boolean" name="useNumCysteines"/>
    <variable type="Boolean" name="useNumOfEachResidue"/>
    
    <variable type="Boolean" name="aaGlobalHistogram"/>
    <variable type="Boolean" name="aaDimericProfile"/>

    <variable type="Boolean" name="pssmGlobalHistogram"/>
    <variable type="Boolean" name="pssmDimericProfile"/>

    <variable type="Boolean" name="ss3GlobalHistogram"/>
    <variable type="Boolean" name="ss3DimericProfile"/>

    <variable type="Boolean" name="drGlobalHistogram"/>
    <variable type="Boolean" name="drDimericProfile"/>

    <variable type="Boolean" name="saGlobalHistogram"/>
    <variable type="Boolean" name="saDimericProfile"/>

    <!-- Residue Parameters -->
    <variable type="Boolean" name="usePosition"/>
    <variable type="Boolean" name="useRelativePosition"/>

    <variable type="PositiveInteger" name="aaWindowSize"/>
    <variable type="PositiveInteger" name="aaLocalHistogramSize"/>
    <variable type="PositiveInteger" name="aaLocalDimericProfileSize"/>
    <variable type="PositiveInteger" name="aaSeparationProfileSize"/>
    <variable type="PositiveInteger" name="aaSegmentProfileSize"/>

    <variable type="PositiveInteger" name="pssmWindowSize"/>
    <variable type="PositiveInteger" name="pssmLocalHistogramSize"/>
    <variable type="PositiveInteger" name="pssmLocalDimericProfileSize"/>
    <variable type="PositiveInteger" name="pssmSeparationProfileSize"/>
    <variable type="PositiveInteger" name="pssmSegmentProfileSize"/>

    <variable type="PositiveInteger" name="ss3WindowSize"/>
    <variable type="PositiveInteger" name="ss3LocalHistogramSize"/>
    <variable type="PositiveInteger" name="ss3LocalDimericProfileSize"/>
    <variable type="PositiveInteger" name="ss3SeparationProfileSize"/>
    <variable type="PositiveInteger" name="ss3SegmentProfileSize"/>

    <variable type="PositiveInteger" name="drWindowSize"/>
    <variable type="PositiveInteger" name="drLocalHistogramSize"/>
    <variable type="PositiveInteger" name="drLocalDimericProfileSize"/>
    <variable type="PositiveInteger" name="drSeparationProfileSize"/>
    <variable type="PositiveInteger" name="drSegmentProfileSize"/>

    <variable type="PositiveInteger" name="saWindowSize"/>
    <variable type="PositiveInteger" name="saLocalHistogramSize"/>
    <variable type="PositiveInteger" name="saLocalDimericProfileSize"/>
    <variable type="PositiveInteger" name="saSeparationProfileSize"/>
    <variable type="PositiveInteger" name="saSegmentProfileSize"/>
  </class>
  



  <!-- BEGIN Model Function -->

  <class name="NumOfEachResidueFunction" base="SimpleUnaryFunction"/>

  <class name="EnsureProteinTargetIsComputedFunction" base="SimpleUnaryFunction">
    <variable type="VariablesEnumeration[Protein]" name="target"/>
  </class>

  <!-- END Model Function -->

  
  <!-- BEGIN Dimeric Functions -->

  <class name="DimericCompositionFunction" base="Function" abstract="yes">
    <variable type="PositiveInteger" name="numEnumerationElements"/>
  </class>
  <class name="AminoAcidDimericCompositionFunction" base="DimericCompositionFunction"/>
  <class name="DoubleVectorDimericCompositionFunction" base="DimericCompositionFunction"/>
  <class name="ProbabilityDimericCompositionFunction" base="DimericCompositionFunction"/>
  <class name="DimericCompositionProxyFunction" base="ProxyFunction"/>

  <class name="LocalDimericCompositionFunction" base="Function" abstract="yes">
    <variable type="PositiveInteger" name="windowSize"/>
    <variable type="PositiveInteger" name="numEnumerationElements"/>
  </class>
  <class name="AminoAcidLocalDimericCompositionFunction" base="LocalDimericCompositionFunction"/>
  <class name="DoubleVectorLocalDimericCompositionFunction" base="LocalDimericCompositionFunction"/>
  <class name="ProbabilityLocalDimericCompositionFunction" base="LocalDimericCompositionFunction"/>
  <class name="LocalDimericCompositionProxyFunction" base="ProxyFunction"/>

  <!-- END Dimeric Functions -->

  <!-- BEGIN Separation Profile Functions -->

  <class name="SeparationProfile" base="Object">
    <variable type="GenericVector[GenericVector[PositiveInteger]]" name="profiles"/>
  </class>
  <class name="CreateSeparationProfileFunction" base="Function" abstract="yes">
    <variable type="PositiveInteger" name="numEnumerationElements"/>
  </class>
  <class name="AminoAcidCreateSeparationProfileFunction" base="CreateSeparationProfileFunction"/>
  <class name="DoubleVectorCreateSeparationProfileFunction" base="CreateSeparationProfileFunction"/>
  <class name="ProbabilityCreateSeparationProfileFunction" base="CreateSeparationProfileFunction"/>
  <class name="CreateSeparationProfileProxyFunction" base="ProxyFunction"/>

  <class name="GetSeparationProfileFunction" base="Function">
    <variable type="PositiveInteger" name="index"/>
    <variable type="PositiveInteger" name="windowSize"/>
  </class>

  <!-- END Separation Profile Functions -->

  <!-- BEGIN Segment Profile Function -->

  <class name="AminoAcidSegmentContainerFunction" base="SegmentContainerFunction"/>
  <class name="DoubleVectorSegmentContainerFunction" base="SegmentContainerFunction"/>
  <class name="ProbabilitySegmentContainerFunction" base="SegmentContainerFunction"/>
  <class name="SegmentContainerProxyFunction" base="ProxyFunction"/>

  <class name="LocalSegmentProfileFunction" base="Function">
    <variable type="PositiveInteger" name="windowSize"/>
    <variable type="PositiveInteger" name="numEnumerationElements"/>
  </class>

  <!-- END Segment Profile Function -->
</library>
