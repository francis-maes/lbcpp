<?xml version="1.0" encoding="UTF-8"?>

<library name="GenericFeatureGenerator" directory="FeatureGenerator/Generic">

  <class name="ConcatenateDoubleFeatureGenerator" base="FeatureGenerator">
    <constructor arguments="bool lazy"/>
  </class>

  <class name="ConcatenateDoubleVectorFeatureGenerator" base="FeatureGenerator">
    <constructor arguments="bool lazy"/>
  </class>

  <class name="ConcatenateFeatureGenerator" base="ProxyFunction">
    <constructor arguments="bool lazy" returnType="Function"/>
    <variable type="Boolean" name="lazy"/>
  </class>

  <class name="ContainerWindowFeatureGenerator" base="FeatureGenerator">
    <constructor arguments="size_t windowSize"/>
    <variable type="PositiveInteger" name="windowSize"/>
  </class>
  
  <class name="MatrixWindowFeatureGenerator" base="FeatureGenerator">
    <constructor arguments="size_t windowRows, size_t windowColumns"/>
    <variable type="PositiveInteger" name="windowRows"/>
    <variable type="PositiveInteger" name="windowColumns"/>
  </class>
  
  <class name="CartesianProductFeatureGenerator" base="FeatureGenerator">
    <constructor arguments="bool lazy"/>
  </class>

</library>
