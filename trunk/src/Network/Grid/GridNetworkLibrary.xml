<?xml version="1.0" encoding="UTF-8"?>

<library name="GridNetwork" directory="Network/Grid">

  <!-- NetworkInterface -->
  <class name="GridNetworkInterface" base="NetworkInterface" abstract="yes"/>
  <class name="ForwarderGridNetworkInterface" base="GridNetworkInterface"/>
  <class name="SgeGridNetworkInterface" base="GridNetworkInterface" shortName="SGE"/>
  <class name="BoincGridNetworkInterface" base="GridNetworkInterface" shortName="BOINC"/>
  
  <!-- NetworkNotification -->
  <class name="GridNetworkNotification" base="NetworkNotification" abstract="yes"/>
  <class name="PushWorkUnitsNotification" base="GridNetworkNotification">
    <variable type="Container[WorkUnitNetworkRequest]" name="networkRequests"/>
  </class>
  <class name="GetFinishedExecutionTraces" base="GridNetworkNotification"/>

</library>
