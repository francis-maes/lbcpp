<?xml version="1.0" encoding="UTF-8"?>

<library name="NetworkWorkUnit" directory="Network/WorkUnit">

  <!-- WorkUnit -->
  <class name="ManagerWorkUnit" base="WorkUnit">
    <variable type="PositiveInteger" name="port" shortName="p"/>
  </class>
  
  <class name="GridWorkUnit" base="WorkUnit">
    <variable type="String" name="gridName" shortName="g"/>
    <variable type="String" name="gridEngine" shortName="e"/>
    <variable type="String" name="hostName" shortName="h"/>
    <variable type="PositiveInteger" name="port" shortName="p"/>
  </class>
  
  <class name="DumbWorkUnit" base="WorkUnit"/>

  <class name="GetTraceWorkUnit" base="WorkUnit">
    <variable type="String" name="workUnitIdentifier" shortName="id"/>
    <variable type="String" name="hostName" shortName="h"/>
    <variable type="PositiveInteger" name="port" shortName="p"/>
  </class>

  <!-- xxx revolution -->
  <class name="XxxServerWorkUnit" base="WorkUnit">
    <variable type="String" name="serverType" shortName="s" description="Type of server: local"/>
    <variable type="PositiveInteger" name="port" shortName="p" description="port"/>
  </class>

  <class name="XxxClientWorkUnit" base="WorkUnit"/>

</library>
