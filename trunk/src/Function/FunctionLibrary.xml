<?xml version="1.0" encoding="UTF-8"?>

<library name="Function" directory="Function">
  <include file="lbcpp/Function/ObjectiveFunction.h"/>

  <class name="Function" abstract="yes">
    <variable type="Boolean" name="pushIntoStack"/>
  </class>

  <!-- Objective Function -->
  <class name="ObjectiveFunction" base="Function" abstract="yes"/>

  <class name="EvaluateObjectiveFunctionWorkUnit" base="WorkUnit">
    <constructor arguments="const String&amp; name, ObjectiveFunctionPtr objective, const Variable&amp; input, double&amp; result"/>
    <variable type="ObjectiveFunction" name="objective"/>
    <variable type="Variable" name="input"/>
  </class>

  <import name="MiscFunctions"/>
  <import name="ScalarFunction"/>
  <import name="ScalarObjectFunction"/>
  <import name="IterationFunction"/>
  <import name="StoppingCriterion"/>
  <import name="Predicate"/>
  <import name="Evaluator"/>

</library>
