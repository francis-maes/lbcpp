<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinEvaluator" directory="Evaluator">

  <class name="ProteinScoreObject" base="ScoreObject">
    <variable type="GenericVector[ScoreObject]" name="scores"/>
  </class>

  <class name="ProteinLearnerScoreObject" base="ScoreObject">
    <variable type="ScoreObject" name="train"/>
    <variable type="ScoreObject" name="validation"/>
    <variable type="ScoreObject" name="test"/>
    <variable type="PositiveInteger" name="numFeaturesPerResidue"/>
  </class>

  <class name="ProteinEvaluator" base="CompositeEvaluator"/>

</library>
