<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinEvaluator" directory="Evaluator">

  <class name="ProteinScoreObject" base="ScoreObject">
    <variable type="GenericVector[ScoreObject]" name="scores"/>
  </class>

  <class name="ProteinLearnerScoreObject" base="ScoreObject">
    <variable type="ScoreObject" name="train"/>
    <variable type="ScoreObject" name="validation"/>
    <variable type="ScoreObject" name="test"/>
    <variable type="PositiveInteger" name="numFeaturesPerResidue"/>
  </class>

  <class name="ProteinEvaluator" base="CompositeEvaluator"/>
  
  <class name="ContactMapEvaluator" base="SupervisedEvaluator">
    <variable type="PositiveInteger" name="minimumDistance"/>
  </class>
  
  <class name="DisulfidePatternScoreObject" base="ScoreObject">
    <variable type="Double" name="accuracy"/>
    <variable type="Double" name="validGraphAccuracy"/>
  </class>
  
  <class name="DisulfidePatternEvaluator" base="SupervisedEvaluator">
    <variable type="Double" name="threshold"/>
    <variable type="PositiveInteger" name="minimumDistanceFromDiagonal"/>
  </class>
  
  <class name="DisulfidePatternBuilderEvaluator" base="DisulfidePatternEvaluator" abstract="yes"/>
  <class name="NaiveDisulfidePatternBuilderEvaluator" base="DisulfidePatternBuilderEvaluator"/>
  <class name="BestFirstDisulfidePatternBuilderEvaluator" base="DisulfidePatternBuilderEvaluator"/>

</library>
