<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinRosettaDataFeatures" directory="Rosetta/Data/Features">
  
  <class name="GeneralFeatures" base="Object" abstract="yes">
    <variable type="Boolean" name="initialized" />
    <variable type="PositiveInteger" name="numStaticFeatures" />
    <variable type="PositiveInteger" name="numDynamicFeatures" />
    <variable type="DoubleVector" name="features" />
    <variable type="DoubleVector" name="staticFeatures" />
    <variable type="DoubleVector" name="dynamicFeatures" />
  </class>
  
  <class name="SimplePoseFeatures" base="GeneralFeatures">
    <variable type="FeatureGenerator" name="numResiduesFeatureGenerator" />
    <variable type="FeatureGenerator" name="shortDistanceFeatureGenerator" />
    <variable type="FeatureGenerator" name="longDistanceFeatureGenerator" />
    <variable type="FeatureGenerator" name="energyFeatureGenerator" />
  </class>
    
</library>
