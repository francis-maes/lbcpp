<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinRosettaData" directory="Rosetta/Data">

  <!-- import -->
  <import name="ProteinRosettaDataMover"/>
  <import name="ProteinRosettaDataMoverSampler"/>
  
  <!-- class -->
  <class name="Rosetta" base="Object" />
  <class name="Pose" base="Object" />  
  <class name="PoseMover" base="Object" abstract="yes" />
  
  <class name="PoseMoverSampler" base="CompositeSampler">
    <constructor arguments="DiscreteSamplerPtr classSampler, size_t numResidues"/>
    <constructor arguments="size_t numResidues"/>
  </class>
    
  <!-- enumeration -->
  <enumeration name="PoseMoverEnumeration">
    <value name="phipsi"/>
    <value name="shear"/>
    <value name="rigidbody"/>
  </enumeration>
  
</library>
