<?xml version="1.0" encoding="UTF-8"?>

<library name="ProteinRosettaOptimizer" directory="Rosetta/ProteinOptimizer">

  <import name="ProteinRosettaOptimizerStoppingCriterion"/>
  
  <class name="GeneralOptimizerStoppingCriterion" base="Object" abstract="yes">
  </class>

  <class name="GeneralOptimizerParameters" base="Object" abstract="yes">
  </class>
  
  <class name="SimulatedAnnealingParameters" base="GeneralOptimizerParameters">
    <variable type="VariableVector" name="vector"/>
    <variable type="PositiveInteger" name="maxIterations"/>
    <variable type="Double" name="initialTemperature"/>
    <variable type="Double" name="finalTemperature"/>
    <variable type="PositiveInteger" name="numSteps"/>
  </class>
  
  
</library>
