<?xml version="1.0" encoding="UTF-8"?>

<library name="Colo" dynamic="yes">
  
  <class name="ColoObject">
    <variable type="IntegerVector[NewPositiveInteger]" name="sequence"/>
  </class>
  <class name="ColoDomain" base="Domain"/>
  <class name="ColoProblem" base="Problem">
    <variable type="NewFile" name="javaDirectory"/>
    <variable type="NewFile" name="modelDirectory"/>
  </class>
  <class name="ColoSampler" base="Sampler">
    <variable type="DenseDoubleVector[PositiveIntegerEnumeration,NewDouble]" name="probabilities"/>
  </class>
  <class name="ColoSampler2" base="Sampler">
    <variable type="ObjectVector[DenseDoubleVector[PositiveIntegerEnumeration,NewDouble]]" name="probabilities"/>
  </class>

  <class name="ColoSandBox" base="WorkUnit">
    <variable type="NewFile" name="javaDirectory"/>
    <variable type="NewFile" name="modelDirectory"/>
    <variable type="NewPositiveInteger" name="numEvaluations"/>
  </class>
  
</library>
